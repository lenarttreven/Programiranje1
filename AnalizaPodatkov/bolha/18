				$(".pager")
				.append($("<a>", {
					id: "ScrollToTopMB",
				    href: "#",
				    rel : "nofollow",
				    html: "na vrh strani"
				}));

				// Animate scroll to top
				$("#ScrollToTopMB").click(function(e){
					e.preventDefault();
					$("html, body").animate({scrollTop : 0},600, "easeOutCirc");
					return false;
				});
			}
		

