<script type="text/javascript">
/*<![CDATA[*/

			if($("#toolBox-bottom .toolbox").length != 0) {
				$("#toolBox-bottom .toolbox").find(".pager")
				.append($("<a>", {
					id: "ScrollToTop",
				    href: "#",
				    rel : "nofollow",
				    html: "na vrh strani<span class=arrowToTop>&nbsp;</span>"
				}));

				// Animate scroll to top
				$("#ScrollToTop").click(function(e){
					e.preventDefault();
					$("html, body").animate({scrollTop : 0},600, "easeOutCirc");
					return false;
				});
			}


